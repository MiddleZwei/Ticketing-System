<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.s15920.model.Manager" table="manager" schema="mas">
        <id name="managerId">
            <column name="manager_id" sql-type="int(11)"/>
        </id>
        <property name="education">
            <column name="education" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
        <property name="yearsOfExperience">
            <column name="years_of_experience" sql-type="double" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>