<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.s15920.model.Checkin" table="checkin" schema="mas">
        <id name="checkInId">
            <column name="check_in_id" sql-type="int(11)"/>
        </id>
        <property name="hireDate">
            <column name="hire_date" sql-type="date" not-null="true"/>
        </property>
        <property name="fireDate">
            <column name="fire_date" sql-type="date" not-null="true"/>
        </property>
        <property name="employmentId">
            <column name="employment_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>